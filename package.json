    "docs:open": "open docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/room-project/rpc-server.git"
  },
  "author": "Dmitry Dudin <dima.dudin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/room-project/rpc-server/issues"
  },
  "homepage": "https://github.com/room-project/rpc-server#readme",
  "dependencies": {
    "@room-project/rpc-core": "0.3.4",
    "cors": "2.8.5",
    "effector": "21.8.5",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "typedoc": "0.20.30",
    "typescript": "4.2.3"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
